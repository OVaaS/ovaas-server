  az network lb rule create \
  --resource-group kawasakiovaas \
  --name myLoadBalancerRuleWeb \
  --lb-name myLoadBalancer \
  --backend-pool-name myLoadBalancerBEPool \
  --backend-port 9000 \
  --frontend-ip-name loadBalancerFrontEnd \
  --frontend-port 9000 \
  --protocol tcp

